isbn = {4146702232},
issn = {20411723},
journal = {Nature Communications},
number = {1},
pages = {1--11},
pmid = {35945213},
publisher = {Springer US},
title = {{Protection of Omicron sub-lineage infection against reinfection with another Omicron sub-lineage}},
