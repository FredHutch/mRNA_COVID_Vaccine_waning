journal = {Nature Communications},
number = {1},
pages = {1--7},
pmid = {35680872},
publisher = {Springer US},
title = {{Waning effectiveness of the third dose of the BNT162b2 mRNA COVID-19 vaccine}},
volume = {13},
year = {2022}
}
