journal = {New England Journal of Medicine},
number = {24},
pages = {e83},
pmid = {34614327},
title = {{Waning of BNT162b2 Vaccine Protection against SARS-CoV-2 Infection in Qatar}},
volume = {385},
year = {2021}
}
